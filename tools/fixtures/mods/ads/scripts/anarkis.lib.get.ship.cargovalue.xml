<line linenr="013" indent="&#160;"><var>$ware&#160;=&#160;</var><var>$ware.list</var><text>[</text><var>$ware.cnt</var><text>]</text></line>
<line linenr="014" indent="&#160;"><var>$amount&#160;=&#160;</var><var>$ship&#160;-&gt;</var><text>&#160;get&#160;amount&#160;of&#160;ware&#160;</text><var>$ware</var><text>&#160;in&#160;cargo&#160;bay</text></line>
<line linenr="015" indent="&#160;"><var>$price&#160;=&#160;</var><text>get&#160;average&#160;price&#160;of&#160;ware&#160;</text><var>$ware</var></line>
<line linenr="016" indent="&#160;"><var>$total&#160;=&#160;</var><var>$total</var><text>&#160;</text><var>+</var><text>&#160;</text><var>(</var><text>&#160;</text><var>$amount</var><text>&#160;</text><var>*</var><text>&#160;</text><var>$price</var><text>&#160;</text><var>)</var></line>
