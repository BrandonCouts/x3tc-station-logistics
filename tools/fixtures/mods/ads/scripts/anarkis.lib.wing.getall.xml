<line linenr="013" indent=""></line>
<line linenr="014" indent=""><var>$follower&#160;=&#160;</var><var>$leader&#160;-&gt;</var><text>&#160;get&#160;formation&#160;follower&#160;ships</text></line>
<line linenr="015" indent=""><var>$cnt&#160;=&#160;</var><text>&#160;size&#160;of&#160;array&#160;</text><var>$follower</var></line>
<line linenr="016" indent=""><var>while&#160;</var><var>$cnt</var></line>
<line linenr="017" indent="&#160;"><text>dec&#160;</text><var>$cnt&#160;=&#160;</var></line>
<line linenr="018" indent="&#160;"><var>$t&#160;=&#160;</var><var>$follower</var><text>[</text><var>$cnt</var><text>]</text></line>
<line linenr="019" indent="&#160;"><text>append&#160;</text><var>$t</var><text>&#160;to&#160;array&#160;</text><var>$result</var></line>
<line linenr="020" indent=""><text>end</text></line>
<line linenr="021" indent=""><text>return&#160;</text><var>$result</var></line>
