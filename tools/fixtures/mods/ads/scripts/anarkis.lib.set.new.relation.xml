<line linenr="009" indent="&#160;"><var>$new.rel&#160;=&#160;</var><var>Neutral</var></line>
<line linenr="010" indent=""><text>end</text></line>
<line linenr="011" indent=""></line>
<line linenr="012" indent=""><var>$array&#160;=&#160;</var><text>&#160;get&#160;ship&#160;array:&#160;of&#160;race&#160;</text><var>$base.race</var><text>&#160;class/type=</text><var>Ship</var></line>
<line linenr="013" indent=""><var>$cnt&#160;=&#160;</var><text>&#160;size&#160;of&#160;array&#160;</text><var>$array</var></line>
