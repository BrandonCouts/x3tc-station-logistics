<line linenr="016" indent="&#160;"><var>$select&#160;=&#160;</var><var>$array</var><text>[</text><var>$cnt</var><text>]</text></line>
<line linenr="017" indent="&#160;"><var>$select&#160;-&gt;</var><text>&#160;set&#160;relation&#160;against&#160;</text><var>$target.race</var><text>&#160;to&#160;</text><var>$new.rel</var></line>
<line linenr="018" indent=""><text>end</text></line>
<line linenr="019" indent=""></line>
<line linenr="020" indent=""><var>$array&#160;=&#160;</var><text>&#160;get&#160;station&#160;array:&#160;of&#160;race&#160;</text><var>$base.race</var><text>&#160;class/type=</text><var>Station</var></line>
<line linenr="021" indent=""><var>$cnt&#160;=&#160;</var><text>&#160;size&#160;of&#160;array&#160;</text><var>$array</var></line>
<line linenr="022" indent=""><var>while&#160;</var><var>$cnt</var></line>
<line linenr="023" indent="&#160;"><text>dec&#160;</text><var>$cnt&#160;=&#160;</var></line>
<line linenr="024" indent="&#160;"><var>$select&#160;=&#160;</var><var>$array</var><text>[</text><var>$cnt</var><text>]</text></line>
