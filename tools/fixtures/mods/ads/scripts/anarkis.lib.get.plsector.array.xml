<line linenr="024" indent="&#160;"><text>dec&#160;</text><var>$sector.count&#160;=&#160;</var></line>
<line linenr="025" indent="&#160;"><var>$sector&#160;=&#160;</var><var>$sector.array</var><text>[</text><var>$sector.count</var><text>]</text></line>
<line linenr="026" indent="&#160;"><var>$station.array&#160;=&#160;</var><text>&#160;find&#160;station:&#160;sector=</text><var>$sector</var><text>&#160;class&#160;or&#160;type=</text><var>Station</var><text>&#160;race=</text><var>Player</var><text>&#160;flags=</text><var>[Find.Multiple]</var><text>&#160;refobj=</text><var>null</var><text>&#160;maxdist=</text><var>null</var><text>&#160;maxnum=</text><var>100</var><text>&#160;refpos=</text><var>null</var></line>
<line linenr="027" indent="&#160;"><var>$station.count&#160;=&#160;</var><text>&#160;size&#160;of&#160;array&#160;</text><var>$station.array</var></line>
<line linenr="028" indent="&#160;"><var>if&#160;</var><var>$station.count</var><text>&#160;</text><var>&lt;</var><text>&#160;</text><var>$min.station</var></line>
<line linenr="029" indent="&#160;&#160;"><text>remove&#160;element&#160;from&#160;array&#160;</text><var>$sector.array</var><text>&#160;at&#160;index&#160;</text><var>$sector.count</var></line>
<line linenr="030" indent="&#160;"><text>end</text></line>
<line linenr="031" interruptable="@" indent="&#160;"><var>=&#160;</var><text>wait&#160;</text><var>100</var><text>&#160;ms</text></line>
<line linenr="032" indent=""><text>end</text></line>
<line linenr="033" indent=""></line>
<line linenr="034" indent=""><text>return&#160;</text><var>$sector.array</var></line>
