<line linenr="022" indent="&#160;"><var>skip&#160;if&#160;</var><var>$victim.race</var></line>
<line linenr="023" indent="&#160;&#160;"><var>$victim.race&#160;=&#160;</var><var>$victim&#160;-&gt;</var><text>&#160;get&#160;owner&#160;race</text></line>
<line linenr="024" indent="&#160;"><var>if&#160;</var><var>$victim.race</var><text>&#160;</text><var>==</var><text>&#160;</text><var>Neutral&#160;Race</var><text>&#160;</text><var>OR</var><text>&#160;</text><var>$victim.race</var><text>&#160;</text><var>==</var><text>&#160;</text><var>Unknown</var><text>&#160;</text><var>OR</var><text>&#160;</text><var>$victim.race</var><text>&#160;</text><var>==</var><text>&#160;</text><var>Friendly&#160;Race</var><text>&#160;</text><var>OR</var><text>&#160;</text><var>$victim.race</var><text>&#160;</text><var>==</var><text>&#160;</text><var>[OWNER]</var></line>
