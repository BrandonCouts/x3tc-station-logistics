<line linenr="017" indent=""><var>$page.id&#160;=&#160;</var><var>8511</var></line>
<line linenr="018" indent=""></line>
<line linenr="019" indent=""><var>if&#160;</var><var>$ship&#160;-&gt;</var><text>&#160;is&#160;of&#160;class&#160;</text><var>Station</var></line>
<line linenr="020" indent="&#160;"><var>$pilot.name&#160;=&#160;</var><var>$ship&#160;-&gt;</var><text>&#160;get&#160;name</text></line>
<line linenr="021" indent=""><text>else</text></line>
<line linenr="022" indent="&#160;"><var>$pilot.name&#160;=&#160;</var><var>$ship&#160;-&gt;</var><text>&#160;get&#160;pilot&#160;name</text></line>
<line linenr="023" indent="&#160;"><var>skip&#160;if&#160;</var><var>$pilot.name</var></line>
<line linenr="024" indent="&#160;&#160;"><var>$pilot.name&#160;=&#160;</var><var>$ship</var></line>
<line linenr="025" indent=""><text>end</text></line>
<line linenr="026" indent=""></line>
<line linenr="027" indent=""><var>skip&#160;if&#160;not&#160;</var><var>$sound</var></line>
