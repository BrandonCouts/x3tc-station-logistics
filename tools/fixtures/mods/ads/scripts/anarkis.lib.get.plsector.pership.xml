<line linenr="026" indent=""><var>while&#160;</var><var>$sector.count</var></line>
<line linenr="027" indent="&#160;"><text>dec&#160;</text><var>$sector.count&#160;=&#160;</var></line>
<line linenr="028" indent="&#160;"><var>$sector&#160;=&#160;</var><var>$sector.array</var><text>[</text><var>$sector.count</var><text>]</text></line>
<line linenr="029" indent="&#160;"><var>$ship.array&#160;=&#160;</var><text>&#160;find&#160;ship:&#160;sector=</text><var>$sector</var><text>&#160;class&#160;or&#160;type=</text><var>Moveable&#160;Ship</var><text>&#160;race=</text><var>Player</var><text>&#160;flags=</text><var>[Find.Multiple]</var><text>&#160;refobj=</text><var>null</var><text>&#160;maxdist=</text><var>null</var><text>&#160;maxnum=</text><var>100</var><text>&#160;refpos=</text><var>null</var></line>
<line linenr="030" indent="&#160;"><var>$ship.count&#160;=&#160;</var><text>&#160;size&#160;of&#160;array&#160;</text><var>$ship.array</var></line>
<line linenr="031" indent="&#160;"><var>$rnd&#160;=&#160;</var><text>&#160;=&#160;random&#160;value&#160;from&#160;0&#160;to&#160;</text><var>2</var><text>&#160;-&#160;1</text></line>
<line linenr="032" indent="&#160;"><var>if&#160;</var><var>(</var><text>&#160;</text><var>$s.amount</var><text>&#160;</text><var>&lt;</var><text>&#160;</text><var>$ship.count</var><text>&#160;</text><var>)</var><text>&#160;</text><var>OR</var><text>&#160;</text><var>(</var><text>&#160;</text><var>$s.amount</var><text>&#160;</text><var>==</var><text>&#160;</text><var>$ship.count</var><text>&#160;</text><var>AND</var><text>&#160;</text><var>$rnd</var><text>&#160;</text><var>==</var><text>&#160;</text><var>1</var><text>&#160;</text><var>)</var></line>
<line linenr="033" indent="&#160;&#160;"><var>$s.amount&#160;=&#160;</var><var>$ship.count</var></line>
<line linenr="034" indent="&#160;&#160;"><var>$s.sector&#160;=&#160;</var><var>$sector</var></line>
<line linenr="035" indent="&#160;"><text>end</text></line>
<line linenr="036" interruptable="@" indent="&#160;"><var>=&#160;</var><text>wait&#160;</text><var>10</var><text>&#160;ms</text></line>
<line linenr="037" indent=""><text>end</text></line>
<line linenr="038" indent=""></line>
<line linenr="039" indent=""><text>return&#160;</text><var>$s.sector</var></line>
