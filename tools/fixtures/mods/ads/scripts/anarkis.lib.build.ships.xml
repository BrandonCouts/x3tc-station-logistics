<line linenr="004" indent=""></line>
<line linenr="005" indent=""><var>$null&#160;=&#160;</var><var>null</var></line>
<line linenr="006" indent=""></line>
<line linenr="007" indent=""><var>$class.list&#160;=&#160;</var><text>&#160;get&#160;ship&#160;type&#160;array:&#160;maker&#160;race=</text><var>$race</var><text>&#160;class=</text><var>Fighter</var></line>
<line linenr="008" indent=""><var>$class.cnt&#160;=&#160;</var><text>&#160;size&#160;of&#160;array&#160;</text><var>$class.list</var></line>
<line linenr="009" indent=""></line>
<line linenr="010" indent=""><var>$result&#160;=&#160;</var><text>&#160;array&#160;alloc:&#160;size=</text><var>0</var></line>
