<line linenr="015" indent="&#160;&#160;"><var>$new.class&#160;=&#160;</var><var>$class.list</var><text>[</text><var>$rnd</var><text>]</text></line>
<line linenr="016" indent="&#160;"><text>else</text></line>
<line linenr="017" indent="&#160;&#160;"><var>$new.class&#160;=&#160;</var><var>$shiptype</var></line>
<line linenr="018" indent="&#160;"><text>end</text></line>
<line linenr="019" indent="&#160;"></line>
<line linenr="020" indent="&#160;"><var>$rnd&#160;=&#160;</var><text>&#160;=&#160;random&#160;value&#160;from&#160;0&#160;to&#160;</text><var>7</var><text>&#160;-&#160;1</text></line>
<line linenr="021" interruptable="@" indent="&#160;"><var>$ship&#160;=&#160;</var><var>$null&#160;-&gt;</var><text>&#160;call&#160;script&#160;</text><call>anarkis.lib.createship</call><text>&#160;:&#160;</text><text>&#160;Ship&#160;Type=</text><var>$new.class</var><text>&#160;</text><text>&#160;location=</text><var>$sector</var><text>&#160;</text><text>&#160;race=</text><var>$race</var><text>&#160;</text><text>&#160;best&#160;weapon&#160;?=</text><var>$rnd</var><text>&#160;</text><text>&#160;no.ion=</text><var>[FALSE]</var><text>&#160;</text><text>&#160;no&#160;ammo=</text><var>[FALSE]</var></line>
<line linenr="022" indent="&#160;"><text>append&#160;</text><var>$ship</var><text>&#160;to&#160;array&#160;</text><var>$result</var></line>
<line linenr="023" indent="&#160;"><var>=&#160;</var><var>$ship&#160;-&gt;</var><text>&#160;install&#160;</text><var>1</var><text>&#160;units&#160;of&#160;</text><var>Jumpdrive</var></line>
<line linenr="024" indent="&#160;"><var>=&#160;</var><var>$ship&#160;-&gt;</var><text>&#160;install&#160;</text><var>1</var><text>&#160;units&#160;of&#160;</text><var>Transporter&#160;Device</var></line>
<line linenr="025" indent="&#160;"></line>
<line linenr="026" indent="&#160;"><var>$ship&#160;-&gt;</var><text>&#160;set&#160;homebase&#160;to&#160;</text><var>$ship.homebase</var></line>
<line linenr="027" indent="&#160;"></line>
<line linenr="028" indent="&#160;"><var>if&#160;</var><var>$Put.in.Home</var><text>&#160;</text><var>==</var><text>&#160;</text><var>[TRUE]</var></line>
<line linenr="029" indent="&#160;&#160;"><var>$ship&#160;-&gt;</var><text>&#160;put&#160;into&#160;environment&#160;</text><var>$ship.homebase&#160;-&gt;</var></line>
<line linenr="030" indent="&#160;"><text>end</text></line>
<line linenr="031" indent="&#160;"></line>
<line linenr="032" indent=""><text>end</text></line>
<line linenr="033" indent=""></line>
<line linenr="034" indent=""><text>return&#160;</text><var>$result</var></line>
