<line linenr="012" indent="&#160;"><text>dec&#160;</text><var>$ship.count&#160;=&#160;</var></line>
<line linenr="013" indent="&#160;"><var>if&#160;</var><var>$shiptype</var><text>&#160;</text><var>==</var><text>&#160;</text><var>null</var></line>
