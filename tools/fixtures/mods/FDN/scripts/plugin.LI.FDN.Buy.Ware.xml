      <line linenr="085" indent="&#160;&#160;&#160;&#160;"><text>end</text></line>
      <line linenr="086" indent="&#160;&#160;&#160;&#160;"><comment>* DEBUG Message End------------------------------------------------------------------</comment></line>
      <line linenr="087" indent="&#160;&#160;&#160;&#160;"><comment>* Check if wares were bought</comment></line>
      <line linenr="088" indent="&#160;&#160;&#160;&#160;"><var>if </var><text></text><var>$bought &gt; 0 </var><text></text></line>
      <line linenr="089" indent="&#160;&#160;&#160;&#160;&#160;"><text></text><var> =</var><text> </text><var>$factory</var><text>  ->  add </text><var>$bought</var><text> units of </text><var>$ware</var><text></text></line>
      <line linenr="090" indent="&#160;&#160;&#160;&#160;&#160;"><comment>* DEBUG Message Start----------------------------------------------------------------</comment></line>
      <line linenr="091" indent="&#160;&#160;&#160;&#160;&#160;"><var>if </var><text></text><var>$debug </var><text></text></line>
      <line linenr="092" indent="&#160;&#160;&#160;&#160;&#160;&#160;"><var>$txt.debug = </var><text></text><var>'Add ' + $bought + ' of ' + $ware + ' to ' + $factory </var><text></text></line>
      <line linenr="093" indent="&#160;&#160;&#160;&#160;&#160;&#160;"><text>gosub </text><var><var>Debug.Sub</var><text>:</text></var><text></text></line>
      <line linenr="094" indent="&#160;&#160;&#160;&#160;&#160;"><text>end</text></line>
      <line linenr="095" indent="&#160;&#160;&#160;&#160;&#160;"><comment>* DEBUG Message End------------------------------------------------------------------</comment></line>
      <line linenr="096" indent="&#160;&#160;&#160;&#160;&#160;"><var>if </var><text></text><var>$bought == $amount </var><text></text></line>
      <line linenr="097" indent="&#160;&#160;&#160;&#160;&#160;&#160;"><text>return </text><var>null</var><text></text></line>
      <line linenr="098" indent="&#160;&#160;&#160;&#160;&#160;"><text>else</text></line>
      <line linenr="099" indent="&#160;&#160;&#160;&#160;&#160;&#160;"><var>$amount = </var><text></text><var>$amount - $bought </var><text></text></line>
      <line linenr="100" indent="&#160;&#160;&#160;&#160;&#160;&#160;"><comment>* DEBUG Message Start----------------------------------------------------------------</comment></line>
      <line linenr="101" indent="&#160;&#160;&#160;&#160;&#160;&#160;"><var>if </var><text></text><var>$debug </var><text></text></line>
      <line linenr="102" indent="&#160;&#160;&#160;&#160;&#160;&#160;&#160;"><var>$txt.debug = </var><text></text><var>$factory + ' still requires ' + $amount + ' of wares' </var><text></text></line>
      <line linenr="103" indent="&#160;&#160;&#160;&#160;&#160;&#160;&#160;"><text>gosub </text><var><var>Debug.Sub</var><text>:</text></var><text></text></line>
      <line linenr="104" indent="&#160;&#160;&#160;&#160;&#160;&#160;"><text>end</text></line>
      <line linenr="105" indent="&#160;&#160;&#160;&#160;&#160;&#160;"><comment>* DEBUG Message End------------------------------------------------------------------</comment></line>
      <line linenr="106" indent="&#160;&#160;&#160;&#160;&#160;"><text>end</text></line>
      <line linenr="107" indent="&#160;&#160;&#160;&#160;"><text>end</text></line>
      <line linenr="108" indent="&#160;&#160;&#160;"><text>end</text></line>
      <line linenr="109" indent="&#160;&#160;"><text>end</text></line>
      <line linenr="110" indent="&#160;&#160;" interruptable="@"><text></text><var> =</var><text> wait randomly from </text><var>300</var><text> to </text><var>600</var><text> ms</text></line>
      <line linenr="111" indent="&#160;"><text>end</text></line>
      <line linenr="112" indent="&#160;"> </line>
      <line linenr="113" indent=""><text>else</text></line>
      <line linenr="114" indent="&#160;"><comment>* debug</comment></line>
      <line linenr="115" indent="&#160;"><var>if </var><text></text><var>$debug </var><text></text></line>
      <line linenr="116" indent="&#160;&#160;"><var>$txt.debug = </var><text></text><var>'DC is not set to buy ' + $ware </var><text></text></line>
      <line linenr="117" indent="&#160;&#160;"><text>gosub </text><var><var>Debug.Sub</var><text>:</text></var><text></text></line>
      <line linenr="118" indent="&#160;"><text>end</text></line>
      <line linenr="119" indent=""><text>end</text></line>
