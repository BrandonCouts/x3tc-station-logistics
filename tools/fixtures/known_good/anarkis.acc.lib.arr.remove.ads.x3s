* ===========================================
* Remove ADS ships from an array of ships
* ===========================================
$null = null
$cnt =  size of array $arr
while $cnt
dec $cnt =
$select = $arr[$cnt]
skip if not $select -> get local variable: name='anarkis.acc.ship'
remove element from array $arr at index $cnt
end

return $arr
