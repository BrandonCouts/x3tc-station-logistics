$ar = $sector -> get ship array from sector/ship/station
$cnt =  size of array $ar
while $cnt
dec $cnt =
$select = $ar[$cnt]
skip if not $select -> is civilian ship
START $select -> call script !ship.cmd.destroy.std :
end
return null
