#name: anarkis.lib.guild.isguild
#lang: 44
#origin_mod: ADS
#source: mods/ADS/scripts/anarkis.lib.guild.isguild.xml

$global.name = sprintf: fmt='anarkis.guild.%s', $plugin.name, null, null, null, null
$array = get global variable: name=$global.name
$xp = $array[3]
if $xp == [TRUE]
return [TRUE]
else
return [FALSE]
end
